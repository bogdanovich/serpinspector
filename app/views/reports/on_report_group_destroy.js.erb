$("#report_report_group_id option:selected").remove();
$("#report_report_group_id")[0].selectedIndex = 0;
$('#reports_list').html("<%= escape_javascript(render(:partial => 'reports_list', :object => @reports)) %>");
$('#report').html("<%= escape_javascript(render(:partial => 'report', :object => @report_items)) %>");
$('#selected_report_group').effect('highlight', {color: '#EDE02D'}, 1000);
$('#selected_report').effect('highlight', {color: '#EDE02D'}, 1000);
$('#notice').html("<%= escape_javascript(flash[:notice]) %>").hide();
$('#notice').fadeIn();

<% flash.discard %>
